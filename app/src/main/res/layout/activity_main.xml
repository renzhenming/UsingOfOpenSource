        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="testRetrofit"
            android:text="retrofit"
            android:textAllCaps="false" />

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="testARouter"
            android:text="ARouter"
            android:textAllCaps="false" />


    </LinearLayout>
</ScrollView>
